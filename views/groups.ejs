<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Groups</title>
  </head>
  <body>
    <table>
      <tr>
        <th>ID</th>
        <th>Group</th>
      </tr>
      <% console.log(rows) %>
      <% for(let i = 0; i < rows.length; i++){ %>
        <tr>
          <td><%= rows[i].id %></td>
          <td><%= rows[i].name_of_group %></td>
          <td><a href="/groups/edit/<%= rows[i].id %>"> Edit </td>
          <td><a href="/groups/delete/<%= rows[i].id %>"> Delete </td>
        </tr>
      <% } %>
        <tr>
        <form class="" action="/groups" method="post">
          <td></td>
          <td><input type="text" name="name_of_group"></td>
          <td><input type="submit" value="Submit"></td>
        </form>
        </tr>
    </table>
  </body>
</html>